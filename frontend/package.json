{
  "name": "frontend",
  "main": "index.tsx",
  "version": "1.0.0",
  "scripts": {
    "start": "APP_ENV=development expo start --clear",
    "start:dev": "APP_ENV=development expo start --clear",
    "start:prod": "APP_ENV=production expo start --clear",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android --variant developmentDebug",
    "android:dev": "cd android && APP_ENV=development ./gradlew installDevelopmentDebug && cd ..",
    "android:dev:launch": "adb shell am start -n com.dokidoki.dev.dokidoki.dev/com.dokidoki.dev.dokidoki.MainActivity",
    "android:dev:release": "dotenv -e .env.development -- bash -c 'cd android && ./gradlew assembleDevelopmentRelease && cd .. && adb install -r android/app/build/outputs/apk/development/release/app-development-release.apk'",
    "android:prod": "cd android && APP_ENV=production ./gradlew installProductionDebug && cd ..",
    "android:prod:launch": "adb shell am start -n com.dokidoki.dev.dokidoki/com.dokidoki.dev.dokidoki.MainActivity",
    "android:prod:release": "dotenv -e .env.production -- bash -c 'cd android && ./gradlew assembleProductionRelease && cd .. && adb install -r android/app/build/outputs/apk/production/release/app-production-release.apk'",
    "ios": "expo run:ios",
    "ios:dev": "APP_ENV=development expo run:ios",
    "ios:prod": "APP_ENV=production expo run:ios",
    "web": "expo start --web",
    "build:dev": "APP_ENV=development eas build --profile development --platform android",
    "build:staging": "APP_ENV=development eas build --profile staging --platform android",
    "build:prod": "APP_ENV=production eas build --profile production --platform android",
    "lint": "expo lint"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/elements": "^2.3.8",
    "@react-navigation/native": "^7.1.6",
    "@react-navigation/stack": "^7.4.2",
    "@reduxjs/toolkit": "^2.8.2",
    "@supabase/supabase-js": "^2.58.0",
    "eas": "^0.1.0",
    "expo": "~53.0.23",
    "expo-blur": "~14.1.5",
    "expo-clipboard": "~7.1.5",
    "expo-constants": "~17.1.7",
    "expo-device": "~7.1.4",
    "expo-font": "~13.3.2",
    "expo-haptics": "~14.1.4",
    "expo-image": "~2.4.1",
    "expo-image-picker": "~16.1.4",
    "expo-linking": "~7.1.7",
    "expo-notifications": "~0.31.4",
    "expo-router": "~5.1.7",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-symbols": "~0.4.5",
    "expo-system-ui": "~5.0.11",
    "expo-updates": "~0.28.17",
    "expo-web-browser": "~14.2.0",
    "firebase": "^11.10.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.6",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-image-crop-picker": "^0.50.1",
    "react-native-image-picker": "^8.2.1",
    "react-native-modal": "^14.0.0-rc.1",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-web": "~0.20.0",
    "react-native-webview": "13.13.5",
    "react-redux": "^9.2.0",
    "redux": "^5.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/react": "~19.0.10",
    "@types/react-redux": "^7.1.34",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~9.2.0",
    "react-native-dotenv": "^3.4.11",
    "typescript": "~5.8.3"
  },
  "private": true
}
